#include<stdio.h>
void main()
{
struct student
{
int roll;
char name[50];
};
struct student s1={1,"yash"};
struct student s2;
s2=s1;
printf("%d %s",s2.roll,s2.name);
if((s1.roll==s2.roll)&&(strcmp(s1.name,s2.name)==0))
{
printf("Both records are same");
}
else
{
printf("Both records are not same");
}
getch();
}
