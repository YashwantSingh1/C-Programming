#include<stdio.h>
#include<conio.h>
void main()
{
    int x[10],i,sum=0;
    printf("Enter the elements of the Array \n");
    for(i=0;i<10;i++)
    {
        scanf("%d",&x[i]);
    }
    for (i=0;i<10;i++)
    {
        sum=sum+x[i];
        printf("sum of digits = %d\n",sum);

    }
    getch();

}
